{
  "name": "boilerplate-react-bootstrap-webpack",
  "version": "0.0.1",
  "description": "reactjs react-bootstrap webpack boilerplate",
  "main": "main.js",
  "scripts": {
    "build": "BUILD_DEV=1 webpack --config webpack.build.config.js",
    "dev": "BUILD_DEV=1 webpack-dev-server --port 5050 --devtool eval --progress --colors --hot --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lionkeng/boilerplate-react-bootstrap-webpack.git"
  },
  "keywords": [
    "reactjs",
    "react-bootstrap",
    "webpack"
  ],
  "author": "lionkeng@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lionkeng/boilerplate-react-bootstrap-webpack/issues"
  },
  "homepage": "https://github.com/lionkeng/boilerplate-react-bootstrap-webpack",
  "dependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.4.0",
    "imagemin": "^3.2.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.1",
    "scriptjs": "^2.5.7",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.9.0"
  }
